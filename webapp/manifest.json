{
	"_version": "1.12.0",
	"sap.app": {
		"id": "ERPFrontendUI5",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
	  		"version": "1.0.0"
	}
},
  	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
  	"sap.ui5": {
		"rootView": {
			"viewName": "ERPFrontendUI5.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
	  		"minUI5Version": "1.60",
	  		"libs": {
				"sap.m": {}
	  		}
		},
		"models": {
	  		"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
		  			"bundleName": "ERPFrontendUI5.i18n.i18n",
		  			"supportedLocales": ["de", "en"],
		  			"fallbackLocale": "de"
				}
	  		}
		}
  	}
}